{
    "shared": {
        "resourceName": "storyvodstreams"
    },
    "template": {
        "name": "Amplify_Video_HLS.json"
    },
    "contentDeliveryNetwork": {
        "enableDistribution": false
    },
    "parameters": {
        "authRoleName": {
            "Ref": "AuthRoleName"
        },
        "GraphQLAPIId": {
            "Fn::GetAtt": [
                "apistorycms",
                "Outputs.GraphQLAPIIdOutput"
            ]
        },
        "GraphQLEndpoint": {
            "Fn::GetAtt": [
                "apistorycms",
                "Outputs.GraphQLAPIEndpointOutput"
            ]
        }
    },
    "permissions": {
        "permissionSchema": [
            "admin"
        ]
    },
    "cms": {
        "overrideSchema": false,
        "editAPI": true
    }
}